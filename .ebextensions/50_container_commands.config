container_commands:
 40translations:
   command: "django-admin compilemessages"
 50collectstatic:
   command: "django-admin collectstatic --noinput"
   leader_only: true
 60migrate:
   command: "django-admin migrate --noinput"
   leader_only: true
 70createcachetable1:
   command: "django-admin createcachetable"
   leader_only: true
option_settings:
 aws:elasticbeanstalk:application:environment:
   DJANGO_SETTINGS_MODULE: "hat.settings"
   PYTHONPATH: "/var/app/current:$PYTHONPATH"
 aws:elasticbeanstalk:container:python:
   WSGIPath: "hat.wsgi:application"
